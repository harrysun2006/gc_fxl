<?xml version="1.0"?>
<flex-config>
	<compiler>
		<accessible>false</accessible>
		<allow-source-path-overlap>true</allow-source-path-overlap>
		<external-library-path>
		   <path-element>${flexlib}\libs\player\10\playerglobal.swc</path-element>
		   <path-element>${flexlib}\libs\</path-element>
		</external-library-path>
		<!--locale>
		   <locale-element>en_US</locale-element>
		</locale-->
		<source-path>
		   <path-element>src</path-element>
		</source-path>
		<!--source-path>
		   <path-element>locale/{locale}</path-element>
		</source-path-->
    <!--namespaces>
			<namespace>
				<uri>http://www.adobe.com/2006/mxml</uri>
				<manifest>manifest.xml</manifest>
			</namespace>
    </namespaces-->
		<strict>true</strict>
		<as3>true</as3>
		<es>false</es>
		<optimize>true</optimize>
		<verbose-stacktraces>false</verbose-stacktraces>
	</compiler>
	<output>fxl-1.0.0.swc</output>
	<include-classes>
		<class>fxl.biff.BIFFReader</class>
		<class>fxl.biff.BIFFVersion</class>
		<class>fxl.biff.BIFFWriter</class>
		<class>fxl.biff.Record</class>
		<class>fxl.cdf.CDFReader</class>
		<!--class>fxl.cdf.Directory</class-->
		<class>fxl.xls.formula.Formula</class>
		<class>fxl.xls.formula.Functions</class>
		<class>fxl.xls.formula.Tokens</class>
		<class>fxl.xls.style.XFormat</class>
		<class>fxl.xls.Cell</class>
		<class>fxl.xls.Formatter</class>
		<class>fxl.xls.Type</class>
		<class>fxl.xls.Workbook</class>
		<class>fxl.xls.Worksheet</class>
	</include-classes>
	<!--include-file>
		<name>assets/pan/pan-e.png</name>
		<path>assets/pan/pan-e.png</path>
	</include-file>
	<include-file>
		<name>assets/pan/pan-ne.png</name>
		<path>assets/pan/pan-ne.png</path>
	</include-file-->
</flex-config>